@import "./multiSelect.wxss";
@import "./common/ntinlinepicker.wxss";
.dep-num-c {
  font-size: 26rpx;
  color: #86a5e6;
  font-weight: bold;
  text-align: center;
}
.dep-num-cd {
  font-size: 26rpx;
  color: #86a5e6;
  font-weight: bold;
  text-align: center;
  margin-top: 10rpx;
}
.dep-tip-c {
  font-size: 26rpx;
  color: #555555;
  font-weight: bold;
  text-align: center;
  margin-top: 15rpx;
}
.dep-3-c {
  position: relative;
  padding: 10rpx 0rpx 20rpx;
  width: 33.33%;
  float: left;
  border-bottom: #eaeaea solid 1rpx;
  height: 110rpx;
}
.dep-3-cd {
  position: relative;
  padding: 10rpx 0rpx 20rpx;
  width: 33.33%;
  float: left;
  border-bottom: #eaeaea solid 1rpx;
  height: 170rpx;
}
.dep-3-c-bd::after {
  content: '';
  position: absolute;
  display: block;
  width: 2rpx;
  right: 0rpx;
  top: 0rpx;
  height: 100%;
  background: #ededed;
}
.common-pannel {
  padding: 10rpx 35rpx;
  overflow: hidden;
  background: #fff;
  border-radius: 10rpx;
}
.common-pannel-vi {
  overflow: visible;
  background: #fff;
  border-radius: 10rpx;
  padding: 10rpx 35rpx;
}
.dep-3-c-bd-n::after {
  background: none;
}
.dep-lead-sa-vi {
  font-size: 26rpx;
  padding: 40rpx 0rpx;
}
.dep-line-vi {
  position: relative;
  color: #999;
  margin-bottom: 20rpx;
  line-height: 20px;
}
.prec-tab-item-ar {
  position: relative;
  padding: 10rpx 0rpx 20rpx 0rpx;
  border-bottom: #d5d7dd solid 1rpx;
  text-align: center;
  font-size: 32rpx;
  color: #999;
  transition: 200ms linear all;
}
.prec-tab-box {
  margin-bottom: 15px;
  margin-top: 20px;
  overflow: hidden;
}
.dep-date-item {
  float: left;
  width: 300rpx;
}
.dep-label {
  float: left;
}
